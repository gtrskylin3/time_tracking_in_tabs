{
  "manifest_version": 3,
  "name": "Time Tracker Pro",
  "version": "2.0",
  "description": "Продвинутое отслеживание времени на сайтах с аналитикой по дням и неделям",
  "permissions": ["tabs", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Time Tracker Pro"
  }
}
